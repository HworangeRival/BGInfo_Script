Function GetEarthquakesLast24Hours()
    Dim url, xhr, json, quakes
    url = "https://earthquake.usgs.gov/earthquakes/feed/v1.0/summary/all_day.geojson"
    Set xhr = CreateObject("MSXML2.XMLHTTP")
    xhr.Open "GET", url, False
    xhr.Send
    json = xhr.responseText
    quakes = Split(json, """type"":""Feature""")
    GetEarthquakesLast24Hours = UBound(quakes)
End Function

echo GetEarthquakesLast24Hours


